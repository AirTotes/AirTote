<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="AirTote.Pages.kounan"
	Title="RJBK　岡南空港">
	<StackLayout>
		<Button
			Text="飛行場施設使用届"
			FontSize="Medium"
			Clicked="kounanAirportUse_Clicked"/>
		<Button
			Text="離着陸訓練許可申請書"
			FontSize="Medium"
			Clicked="kounandepart_Clicked"/>
		<Button
			Text="着陸料等減免申請書"
			FontSize="Medium"
			Clicked="landingdiscount_Clicked"/>
		<Button
			Text="空港施設の仕様に係る条件の確認書"
			FontSize="Medium"
			Clicked="AirportCheck_Clicked"/>
	</StackLayout>
</ContentPage>