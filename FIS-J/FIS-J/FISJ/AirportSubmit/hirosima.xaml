<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FIS_J.FISJ.hirosima"
             Title="RJOA　広島空港">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="hirosimaAirportUse"
                    Text="空港使用届"
                    FontSize="Medium"
                    Clicked="hirosimaAirportUse_Clicked"/>
            <Button x:Name="landingdiscount"
                    Text="着陸料免除申請書"
                    FontSize="Medium"
                    Clicked="landingdiscount_Clicked"/>
            <Button x:Name="PilotInformation"
                    Text="運行者情報届出書"
                    FontSize="Medium"
                    Clicked="PilotInformation_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>