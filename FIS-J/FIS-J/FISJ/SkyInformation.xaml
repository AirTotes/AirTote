<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FIS_J.FISJ.SkyInformation">
    <ContentPage.Content>
        <ScrollView>
            <StackLayout>
                <Button x:Name="badwetherforecast"
                Text="国内悪天予想図"
                BackgroundColor="LightSteelBlue"
                TextColor="Black"
                Font="20"/>
                <Button x:Name="lowerbadwether"
                Text="下層悪天予想図"
                BackgroundColor="LightSteelBlue"
                TextColor="Black"
                Font="20"/>
                <Button x:Name="signmet"
                Text="シグメット情報"
                BackgroundColor="LightSteelBlue"
                TextColor="Black"
                Font="20"/>
                <Button x:Name="Internationalfukuoka"
                Text="全国航空気象解説報(福岡FIR)"
                BackgroundColor="LightSteelBlue"
                TextColor="Black"
                Font="20"/>
                <Button x:Name="InternationalDomestic"
                Text="全国航空気象解説報(国内空域)"
                BackgroundColor="LightSteelBlue"
                TextColor="Black"
                Font="20"/>
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>