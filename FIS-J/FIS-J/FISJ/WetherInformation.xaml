<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FIS_J.FISJ.WetherInformation">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="Nomwether"
                Text="数値天気予報図"
                BackgroundColor="LightSteelBlue"
                
                Font="25"/>
            <Button x:Name="Upperwether"
                Text="高層天気図"
                BackgroundColor="LightSteelBlue"
                
                Font="25"/>
            <Button x:Name="SkyInformation"
                Text="空域の情報"
                BackgroundColor="LightSteelBlue"
                
                Font="25"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>