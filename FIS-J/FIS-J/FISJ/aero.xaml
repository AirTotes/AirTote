<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FIS_J.FISJ.aero"
             Title="航空情報">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="AIPView"
                    Text="AIP"
                    FontSize="Medium"/>
            <WebView/>
            <Button x:Name="SUPsView"
                    Text="SUPs"
                    FontSize="Medium"/>
            <Button x:Name="AICsView"
                    Text="AICs"
                    FontSize="Medium"/>
            <WebView x:Name="webView" VerticalOptions="FillAndExpand" Source="{Binding HTML}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>

