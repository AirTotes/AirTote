<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
	xmlns="http://xamarin.com/schemas/2014/forms"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="FIS_J.FISJ.aero"
	Title="航空情報">
	<ContentPage.Content>
		<StackLayout>
			<Button
				Clicked="AIPView_Clicked"
				Text="AIP"
				FontSize="Medium"/>
			<Button
				Clicked="SUPsView_Clicked"
				Text="SUPs"
				FontSize="Medium"/>
			<Button
				Clicked="AICsView_Clicked"
				Text="AICs"
				FontSize="Medium"/>
			<WebView
				VerticalOptions="FillAndExpand"
				Source="{Binding HTML}"/>
		</StackLayout>
	</ContentPage.Content>
</ContentPage>

