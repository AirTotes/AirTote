<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
	xmlns="http://xamarin.com/schemas/2014/forms"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="FIS_J.FISJ.SkyInformation"
	Title="空域の情報">
	<ScrollView>
		<StackLayout>
			<Button
				Clicked="Badwetherforecast_Clicked"
				Text="国内悪天予想図"
				Font="20"/>
			<Button
				Clicked="lowerbadwether_Clicked"
				Text="下層悪天予想図"
				Font="20"/>
			<Button
				Clicked="signmet_Clicked"
				Text="シグメット情報"
				Font="20"/>
			<Button
				Clicked="Internationalfukuoka_Clicked"
				Text="全国航空気象解説報(福岡FIR)"
				Font="20"/>
			<Button
				Clicked="InternationalDomestic_Clicked"
				Text="全国航空気象解説報(国内空域)"
				Font="20"/>
		</StackLayout>
	</ScrollView>
</ContentPage>