<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="FIS_J.FISJ.haneda"
	Title="RJAA　羽田空港">
	<StackLayout>
		<Button
			Text="空港使用届出書"
			FontSize="Medium"
			Clicked="hanedaAirportuse_Clicked"/>
		<Button
			Text="空港使用許可申請書"
			FontSize="Medium"
			Clicked="hanedaAirportusepermittion_Clicked"/>
		<Button
			Text="制限重量超過許可申請書"
			FontSize="Medium"
			Clicked="hanedaWeightOverpermittion_Clicked"/>
		<Button
			Text="空港内土地・建物・設備使用許可申請書"
			FontSize="Medium"
			Clicked="hanedaUseInstitutionpermmition_1_Clicked"/>
		<Button
			Text="施設・設備設置許可申請書"
			FontSize="Medium"
			Clicked="hanedaUseInstitutionpermmition_2_Clicked"/>
		<Button
			Text="施設・設備設置許可変更申請書"
			FontSize="Medium"
			Clicked="hanedaUseInstitutionpermmition_3_Clicked"/>
		<Button
			Text="空港使用料減免申請書"
			FontSize="Medium"
			Clicked="hanedaDiscount_Clicked"/>
		<Button
			Text="調布飛行場制限表面図"
			FontSize="Medium"
			Clicked="hanedarestrictmap_Clicked"/>
	</StackLayout>
</ContentPage>